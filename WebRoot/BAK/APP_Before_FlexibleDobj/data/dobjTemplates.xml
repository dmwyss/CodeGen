<?xml version="1.0" encoding="UTF-8"?>
<templates>
	<template id="sqlCreateTable" title="SQL Create Table">
		<formula><![CDATA[
$SECTION{norepeat}<table cellpadding="0" cellspacing="0" border="0" class="" style="">
<!-- ${static.entityName} -->
<!-- ${static.javaClassPath} -->
$SECTION{}
	<tr>
		<td>${id}</td>
		<td>${type}</td>
		<td>$REPLACE{${type}$String$VARCHAR}</td>
		<td>${description}</td>
	</tr>
$SECTION{norepeat}
</table>
]]>		</formula>
	</template>
	<template id="htmlFormTable" title="HTML Form Table">
		<formula><![CDATA[
$SECTION{norepeat}<table cellpadding="0" cellspacing="0" border="0" class="" style="">
<!-- tableName=${entityName} -->
<!-- javaClassPath=${javaClassPath} -->
$SECTION{}
	<tr>
		<td>${0}</td>
		<td>${1}</td>
		<td>${2}</td>
		<td>${3}</td>
	</tr>
$SECTION{norepeat}
</table>
]]>		</formula>
	</template>
</templates>
